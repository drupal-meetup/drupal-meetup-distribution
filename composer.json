{
    "name": "drupal-meetup/drupal-meetup-distribution",
    "description": "A distribution for building meetup event websites with Drupal.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/opdavies/meetup-drupal-distribution",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "drupal/core": "^9.1",
        "drush/drush": ">=10"
    },
    "require-dev": {
        "drupal/core-dev": "^9.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {}
    }
}

